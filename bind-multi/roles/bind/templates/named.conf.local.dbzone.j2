$TTL    604800
@       IN      SOA     ns1.{{primary}} admin.{{primary}}. (
                  3       ; Serial
             604800     ; Refresh
              86400     ; Retry
            2419200     ; Expire
             604800 )   ; Negative Cache TTL
;
; name servers - NS records
     IN      NS      ns1.{{primary}}.
     IN      NS      ns2.{{primary}}.

; name servers - A records
ns1.{{primary}}.          IN      A       192.168.50.8
ns2.{{primary}}.          IN      A       192.168.50.9

; host - A records
host1.{{primary}}.        IN      A      192.168.50.10
