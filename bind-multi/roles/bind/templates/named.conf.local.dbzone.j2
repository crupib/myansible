$TTL    604800
@       IN      SOA     ns1.{{ primary['dom'] }} admin.{{ primary['ip'] }}. (
                  3       ; Serial
             604800     ; Refresh
              86400     ; Retry
            2419200     ; Expire
             604800 )   ; Negative Cache TTL
;
; name servers - NS records
     IN      NS      ns1.{{primary['dom']}}.
     IN      NS      ns2.{{secondary['dom']}}.

; name servers - A records
{% for x in A_rec %}
{{x.arec}}.{{x.dom}}.      IN      A       {{x.ip}} 
{% endfor %}
